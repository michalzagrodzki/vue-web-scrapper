{
  "name": "vue-web-scrapper",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "client": "vue-cli-service serve --open",
    "server": "DEBUG=express:* nodemon server.js",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "connect-history-api-fallback": "^1.5.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "path": "^0.12.7",
    "request": "^2.85.0",
    "vue": "^2.5.13",
    "vue-axios": "^2.1.1",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "nodemonConfig": {
    "verbose": true
  }
}
